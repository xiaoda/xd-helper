{
  "name": "xdhelper",
  "version": "0.0.7",
  "description": "js helper",
  "scripts": {
    "start": "NODE_ENV='dev' webpack-dev-server --config webpack.dev.config.js",
    "build": "NODE_ENV='prod' webpack --config webpack.build.config.js",
    "clear": "rm -rf dist",
    "test": "node_modules/mocha/bin/mocha",
    "pub": "npm version patch && npm run copypackfiles && cd dist && npm publish",
    "prepub": "npm run clear && npm run build && npm run test",
    "copypackfiles": "/bin/cp package.json dist/package.json && /bin/cp README.md dist/README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoda/xdhelper.git"
  },
  "keywords": [
    "js",
    "helper",
    "xdhelper"
  ],
  "author": "xiaoda",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-stage-1": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "glob": "^7.1.1",
    "mocha": "^3.4.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
